<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/padding_40">

        <TextView
            android:id="@+id/tv_num"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="1"
            android:text=""
            android:gravity="center"
            android:layout_width="@dimen/padding_0"
            android:layout_height="match_parent"/>


        <TextView
            android:id="@+id/tv_project"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="2"
            android:text=""
            android:gravity="center"
            android:layout_width="@dimen/padding_0"
            android:layout_height="match_parent"/>

        <TextView
            android:id="@+id/tv_code"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="2"
            android:text=""
            android:gravity="center"
            android:layout_width="@dimen/padding_0"
            android:layout_height="match_parent"/>

        <TextView
            android:id="@+id/tv_name"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="2"
            android:text=""
            android:gravity="center"
            android:layout_width="@dimen/padding_0"
            android:layout_height="match_parent"/>



        <TextView
            android:id="@+id/tv_techStatus"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="2"
            android:text=""
            android:gravity="center"
            android:layout_width="@dimen/padding_0"
            android:layout_height="match_parent"/>


        <TextView
            android:id="@+id/tv_approver"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="2"
            android:text=""
            android:gravity="center"
            android:layout_width="@dimen/padding_0"
            android:layout_height="match_parent"/>


        <TextView
            android:id="@+id/tv_approvalDate"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="2"
            android:text=""
            android:gravity="center"
            android:layout_width="@dimen/padding_0"
            android:layout_height="match_parent"/>



        <TextView
            android:id="@+id/tv_issl"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="2"
            android:text=""
            android:gravity="center"
            android:layout_width="@dimen/padding_0"
            android:layout_height="match_parent"/>


        <TextView
            android:id="@+id/tv_conclusion"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="3"
            android:text=""
            android:gravity="center"
            android:layout_width="@dimen/padding_0"
            android:layout_height="match_parent"/>

        <TextView
            android:id="@+id/tv_description"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="2"
            android:text=""
            android:gravity="center"
            android:layout_width="@dimen/padding_0"
            android:layout_height="match_parent"/>


        <TextView
            android:id="@+id/tv_caozuo"
            android:background="@drawable/shape_text"
            android:textSize="@dimen/text_font_13"
            android:layout_weight="2"
            android:text="操作"
            android:gravity="center"
            android:layout_width="0dp"
            android:layout_height="@dimen/padding_30"/>

    </LinearLayout>
    <View
        android:background="@color/color_D0D0D0"
        android:layout_width="match_parent"
        android:layout_height="@dimen/padding_1"/>

</LinearLayout>